expression FxReadCSVFile =
		( TableType as text, FileName as text ) as table =>
		    let
		        FileContents = File.Contents ( ContosoFolder & "\" & TableType & "\" & FileName & ".csv" ),
		        CsvDocument = Csv.Document (
		            FileContents,
		            [ Delimiter = ",", Encoding = 1252, QuoteStyle = QuoteStyle.None ]
		        ),
		        PromoteHeaders = Table.PromoteHeaders ( CsvDocument, [ PromoteAllScalars = true ] )
		    in
		        PromoteHeaders
